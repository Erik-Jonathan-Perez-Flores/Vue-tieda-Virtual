 city:''\n        }\n    },\n    computed:{\n        getAvatar(){\n            return this.path!=''?this.path:require('@/assets/logo.jpeg');\n        },\n    },\n    methods:{\n        init(){\n            this.axios({\n                method: 'GET',\n                data:null,\n                url:'/user/business',\n            }).then(response=>response.data[0])\n            .then(resp=>{\n                this.name=resp.name;\n                this.path=resp.path;\n                this.direction=resp.direction;\n                this.phone=resp.phone;\n                this.movil=resp.movil;\n                this.city=resp.city;\n            })\n            .catch(err=>{\n                console.log(err);\n            });\n        }\n    },\n    created(){\n        this.init();\n    }\n}\n</script>"]}]}