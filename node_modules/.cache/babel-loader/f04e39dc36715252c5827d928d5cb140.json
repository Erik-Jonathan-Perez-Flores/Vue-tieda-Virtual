CAtBA;AAyBA,EAAA,OAAA,kCACA,YAAA,CAAA,CAAA,aAAA,CAAA,CADA;AAEA,IAAA,QAFA,sBAEA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,WAAA;AACA,KAJA;AAKA,IAAA,SALA,uBAKA;AAAA;;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,EAAA;AACA,YAAA,IAAA,GAAA;AACA,UAAA,KAAA,EAAA,KAAA,KADA;AAEA,UAAA,IAAA,EAAA,KAAA;AAFA,SAAA;AAIA,aAAA,KAAA,CAAA;AACA,UAAA,MAAA,EAAA,MADA;AAEA,UAAA,IAAA,EAAA,EAAA,CAAA,SAAA,CAAA,IAAA,CAFA;AAGA,UAAA,GAAA,EAAA;AAHA,SAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;;AACA,UAAA,KAAA,CAAA,WAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,SATA,EASA,KATA,CASA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,IAAA;;AACA,cAAA,GAAA,CAAA,QAAA,EAAA;AACA,YAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,WAFA,MAEA;AACA,YAAA,KAAA,CAAA,OAAA,GAAA,uBAAA;AACA;AACA,SAhBA;AAiBA,OAtBA,MAsBA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA;AAEA,KAhCA;AAiCA,IAAA,EAjCA,gBAiCA;AACA,UAAA,KAAA,KAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA;AAtCA,IAzBA;AAiEA,EAAA,OAjEA,qBAiEA;AACA,QAAA,KAAA,OAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA;AArEA,CAAA","sourcesContent":["<template>\n    <v-container fluid fill-height style=\"background: linear-gradient(90deg, rgba(226,221,221,1) 0%, rgba(103,125,172,0.44441526610644255) 23%, rgba(21,101,192,1) 88%);\">\n        <v-row justify=\"center\">\n            <v-card elevation=\"15\" width=\"400\" style=\"border-radius:2em\">\n                <v-card-text>\n                    <v-form v-model=\"valid\" ref=\"form\">\n                        <v-container>\n                            <v-row>\n                                <v-col cols=\"12\">\n                                    <h1 class=\"primary--text\">Login</h1>\n                                </v-col>\n                                <v-col cols=\"12\">\n                                    <v-text-field\n                                        v-model=\"email\"\n                                        :rules=\"emailRules\"\n                                        label=\"E-mail\"\n                                        required\n                                        dense\n                                    ></v-text-field>\n                                </v-col>\n                                <v-col cols=\"12\">\n                                    <v-text-field\n                                        v-model=\"pass\"\n                                        :rules=\"passRules\"\n                                        label=\"Password\"\n                                        type=\"password\"\n                                        required\n                                        dense\n                                        @keyup.enter=\"sessionGo\"\n                                    ></v-text-field>\n                                </v-col>\n                                <v-col cols=\"12\" sm=\"6\" class=\"d-flex justify-center\">\n                                    <v-btn large color=\"secondary\" class=\"text-capitalize\" @click=\"sessionGo\">Login</v-btn>\n                                </v-col>\n                                <v-col cols=\"12\" sm=\"6\" class=\"d-flex justify-center\">\n                                    <v-btn large color=\"secondary\" class=\"text-capitalize\" @click=\"register\">Registro</v-btn>\n                                </v-col>\n                            </v-row>\n                        </v-container>\n                    </v-form>\n                </v-card-text>\n            </v-card>\n        </v-row>\n        <Dialog\n            :message=\"message\"\n            :dialog=\"dialog\"\n            :redir=\"redir\"\n            @ok-template=\"ok\"\n        />\n    </v-container>\n</template>\n\n<script>\n\nimport qs from 'qs';\nimport Dialog from '@/components/DialogMessage';\nimport {mapState,mapMutations} from 'vuex';\n\nexport default {\n    components:{\n        Dialog\n    },\n    data() {\n        return{\n            valid:false,\n            email: '',\n            emailRules: [\n                v => !!v || 'E-mail es requerido',\n                v => /.+@.+/.test(v) || 'E-mail invalido',\n            ],\n            pass: '',\n            passRules: [\n                v => !!v || 'El password es requerido',\n            ],\n\n            dialog:false,\n            message:'',\n            redir:false,\n        }\n    },\n    computed:{\n        ...mapState(['session']),\n    },\n    methods:{\n        ...mapMutations(['pushSession']),\n        register(){\n            this.$router.push('/register');\n        },\n        sessionGo(){\n            if(this.$refs.form.validate()){\n                let data={\n                    email:this.email, \n                    pass:this.pass,\n                }\n                this.axios({\n                    method: 'POST',\n                    data:qs.stringify(data),\n                    url:'/user/login',\n                }).then(response=>{\n                    this.dialog=true;\n                    this.message=response.data.message+'';\n                    this.redir=response.data.status;\n                    this.pushSession(response.data.data);\n                }).catch(err=>{\n                    this.dialog=true;\n                    if (err.response) {\n                        this.message=err.response.data.message;\n                    }else{\n                        this.message='Error en la respuesta';\n                    }\n                });\n            }else{\n                this.$refs.form.validate();\n            }\n            \n        },\n        ok(){\n            if(this.redir){\n                this.$router.push('/');\n            }\n            this.dialog=false;\n        }\n    },\n    created(){\n        if(this.session){\n            this.$router.push('/');\n        }\n    }\n}\n</script>"],"sourceRoot":"src/views/client"