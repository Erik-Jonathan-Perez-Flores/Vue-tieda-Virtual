AAA,YAAA;AACA,YAAA,cAAA,GAAA,IAAA,CAAA,GAAA,CAAA,MAAA,CAAA,WAAA,EAAA,QAAA,CAAA,eAAA,CAAA,SAAA,EAAA,QAAA,CAAA,IAAA,CAAA,SAAA,IAAA,MAAA,CAAA,WAAA,KAAA,QAAA,CAAA,eAAA,CAAA,YAAA;;AACA,YAAA,cAAA,EAAA;AACA,cAAA,KAAA,CAAA,IAAA,IAAA,KAAA,CAAA,gBAAA,EAAA;AACA,YAAA,KAAA,CAAA,gBAAA,GAAA,KAAA;;AACA,YAAA,KAAA,CAAA,IAAA;AACA;AACA;AACA,OARA;AASA,KAdA;AAeA,IAAA,IAfA,kBAeA;AAAA;;AACA,UAAA,CAAA,KAAA,IAAA,EAAA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,IAAA,EAAA,IAFA;AAGA,QAAA,GAAA,EAAA,oBAAA,KAAA,IAAA,GAAA,SAAA,GAAA,KAAA,KAAA,GAAA,UAAA,GAAA,KAAA;AAHA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA;AAAA,eAAA,QAAA,CAAA,IAAA;AAAA,OAJA,EAKA,IALA,CAKA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,IAAA,IAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,GAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,IAAA,CAAA;AACA,UAAA,MAAA,CAAA,IAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,gBAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA;AACA;;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,OAdA,EAeA,KAfA,CAeA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAjBA;AAkBA,KApCA;AAqCA,IAAA,SArCA,qBAqCA,EArCA,EAqCA;AACA,WAAA,MAAA,GAAA,EAAA,CAAA,IAAA;AACA,WAAA,IAAA,GAAA,CAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA;AA3CA,GArBA;AAkEA,EAAA,OAlEA,qBAkEA;AACA,SAAA,IAAA;AACA;AApEA,CAAA","sourcesContent":["<template>\n    <v-container>\n        <v-row>\n            <v-col cols=\"12\">\n                <h1>Lista de Categorias:</h1>\n            </v-col>\n            <v-col cols=\"12\">\n                <Search\n                    :place=\"'Busca alguna categoría...'\"\n                    @search-text=\"textQuery\"\n                />\n            </v-col>\n            <v-col cols=\"12\" v-if=\"category.length>0\">\n                <v-simple-table>\n                    <template v-slot:default>\n                        <thead>\n                        <tr>\n                            <th class=\"text-left\">\n                                Nro\n                            </th>\n                            <th class=\"text-left\">\n                                Nombre\n                            </th>\n                            <th class=\"text-left\">\n                                Imagen\n                            </th>\n                            <th class=\"text-left\">\n                                Acción\n                            </th>\n                        </tr>\n                        </thead>\n                        <tbody>\n                        <tr\n                            v-for=\"(item,index) in category\"\n                            :key=\"index\"\n                        >\n                            <td>{{index+1}}</td>\n                            <td>{{ item.name }}</td>\n                            <td >\n                                <v-img :src=\"item.path\" width=\"50\" height=\"50\"></v-img>\n                            </td>\n                            <td>\n                                <v-btn text small @click=\"editProduct(item._id)\">\n                                    <v-icon color=\"accent\">{{edit}}</v-icon>\n                                </v-btn>\n                            </td>\n                        </tr>\n                        </tbody>\n                    </template>\n                </v-simple-table>\n            </v-col>\n            <v-col cols=\"12\" class=\"d-flex justify-center\" v-else>\n                <v-icon size=\"200\" color=\"accent\">{{iconDatabase}}</v-icon>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\n\nimport { mdiPencilOutline,mdiTrashCanOutline,mdiDatabaseRefresh } from '@mdi/js';\nimport Search from '@/components/admin/Search';\n\nexport default {\n    components:{\n        Search\n    },\n    data() {\n        return{\n            category:[],\n            edit:mdiPencilOutline,\n            skip:0,\n            limit:10,\n            call:true,\n            scrolledToBottom:true,\n            message:'',\n            iconDatabase:mdiDatabaseRefresh,\n\n            search:'',\n        }\n    },\n    mounted(){\n        this.scroll();\n    },\n    methods:{\n        editProduct(id){\n            this.$router.push('/updatecategory/'+id);\n        },\n        scroll () {\n            window.onscroll = () => {\n                let bottomOfWindow = Math.max(window.pageYOffset, document.documentElement.scrollTop, document.body.scrollTop) + window.innerHeight === document.documentElement.offsetHeight;\n                if (bottomOfWindow) {\n                    if(this.call&&this.scrolledToBottom){\n                        this.scrolledToBottom=false;\n                        this.init();   \n                    }\n                }\n            }\n        },\n        init(){\n            if(!this.call)return;\n            this.call=false;\n            this.axios({\n                method: 'GET',\n                data:null,\n                url:'/category?skip='+this.skip+'&limit='+this.limit+'&search='+this.search,\n            }).then(response=>response.data)\n            .then(resp=>{\n                if(resp&&resp.length>0){\n                    this.category=this.category.concat(resp);\n                    this.skip+=10;\n                    this.scrolledToBottom=true;\n                    this.call = true;\n                    return;\n                }\n                this.call = false;\n            })\n            .catch(err=>{\n                console.log(err);\n            });\n        },\n        textQuery(ob){\n            this.search=ob.text;\n            this.skip=0;\n            this.callApi=true;\n            this.category=[];\n            this.init();\n        }\n    },\n    created(){\n        this.init();\n    },\n}\n</script>"],"sourceRoot":"src/views/admin/data"}]}