AAYA,IAAA,MAZA,kBAYA,GAZA,EAYA;AAAA;;AACA;AACA,UAAA,KAAA,KAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AAEA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,IAAA,EAAA,IAFA;AAGA,QAAA,GAAA,EAAA;AAHA,OAAA,EAKA,IALA,CAKA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,IAAA;AAAA,OALA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA;AACA,OARA,EAQA,KARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAVA,EAUA,OAVA,CAUA;AAAA,eAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AAAA,OAVA;AAWA;AA5BA;AATA,CAAA","sourcesContent":["<template>\n    <v-autocomplete\n        v-model=\"model\"\n        :items=\"items\"\n        :loading=\"isLoading\"\n        :search-input.sync=\"search\"\n        chips\n        clearable\n        hide-details\n        hide-selected\n        outlined\n        dense\n        item-text=\"name\"\n        item-value=\"symbol\"\n        label=\"Busca la categoría...\"\n        solo\n    >\n        <template v-slot:no-data>\n            <v-list-item>\n            <v-list-item-title>\n                Busca alguna \n                <strong>categoría</strong>\n            </v-list-item-title>\n            </v-list-item>\n        </template>\n        <template v-slot:selection=\"{ attr, on, item, selected }\">\n            <v-chip\n                v-bind=\"attr\"\n                :input-value=\"selected\"\n                color=\"accent\"\n                class=\"white--text\"\n                v-on=\"on\"\n            >\n                <v-icon left>\n                    {{icon}}\n                </v-icon>\n                <span v-text=\"item.name\"></span>\n            </v-chip>\n        </template>\n        <template v-slot:item=\"{ item }\">\n            <v-list-item-avatar\n                \n            >\n                <v-img :src=\"item.path\" @error=\"require('@/assets/logo.jpeg')\"></v-img>\n            </v-list-item-avatar>\n            <v-list-item-content>\n                <v-list-item-title v-text=\"item.name\"></v-list-item-title>\n            </v-list-item-content>\n            <v-list-item-action>\n                <v-icon>{{icon}}</v-icon>\n            </v-list-item-action>\n        </template>\n    </v-autocomplete>\n</template>\n\n<script>\n\nimport { mdiShapeOutline } from '@mdi/js';\n\nexport default {\n    data: () => ({\n        isLoading: false,\n        items: [],\n        model: null,\n        search: null,\n        tab: null,\n        icon:mdiShapeOutline\n    }),\n    watch: {\n        model (val) {\n            if (val != null){ \n                this.tab = 0;\n                let index=this.items.findIndex(dat=>{return dat.name==this.model});\n                this.$emit('charge-category',{...this.items[index]});\n            }\n            else {\n                this.tab = null;\n                this.$emit('charge-category',{});\n            }\n        },\n        search (val) {\n            // Items have already been loaded\n            if (this.items.length > 0) return\n\n            this.isLoading = true\n            this.axios({\n                method: 'GET',\n                data:null,\n                url:'/category',\n            })\n            .then(res => res.data)\n            .then(response=>{\n                this.items=response;\n            }).catch(err=>{\n                console.log(err)\n            }).finally(() => (this.isLoading = false));\n        },\n    },\n}\n</script>"],"sourceRoot":"src/components/admin"}]}