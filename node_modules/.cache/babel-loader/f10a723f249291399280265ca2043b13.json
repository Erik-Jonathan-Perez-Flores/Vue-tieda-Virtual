AAeA,MAAA,IAAA,EAAA,EAfA;AAgBA,MAAA,SAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,0BAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eAAA,+EAAA,IAAA,CAAA,CAAA,KAAA,uGAAA;AAAA,OAFA,CAhBA;AAoBA,MAAA,KAAA,EAAA,EApBA;AAqBA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,0BAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eAAA,CAAA,IAAA,KAAA,CAAA,IAAA,IAAA,yBAAA;AAAA,OAFA,CArBA;AA0BA,MAAA,MAAA,EAAA,KA1BA;AA2BA,MAAA,OAAA,EAAA,EA3BA;AA4BA,MAAA,KAAA,EAAA;AA5BA,KAAA;AA8BA,GAnCA;AAoCA,EAAA,QAAA,oBACA,QAAA,CAAA,CAAA,SAAA,CAAA,CADA,CApCA;AAuCA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,oBAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,EAAA;AACA;AACA;AACA,kBAAA,IAHA,GAGA;AACA,oBAAA,IAAA,EAAA,MAAA,CAAA,IADA;AAEA,oBAAA,KAAA,EAAA,MAAA,CAAA,KAFA;AAGA,oBAAA,KAAA,EAAA,MAAA,CAAA,KAHA;AAIA,oBAAA,IAAA,EAAA,MAAA,CAAA;AAJA,mBAHA;;AASA,kBAAA,MAAA,CAAA,KAAA,CAAA;AACA,oBAAA,MAAA,EAAA,MADA;AAEA,oBAAA,IAAA,EAAA,EAAA,CAAA,SAAA,CAAA,IAAA,CAFA;AAGA,oBAAA,GAAA,EAAA;AAHA,mBAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,oBAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,oBAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,mBARA,EAQA,KARA,CAQA,UAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,MAAA,GAAA,IAAA;;AACA,wBAAA,GAAA,CAAA,QAAA,EAAA;AACA,sBAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,qBAFA,MAEA;AACA,sBAAA,MAAA,CAAA,OAAA,GAAA,uBAAA;AACA;AACA,mBAfA;AAiBA,iBA1BA,MA0BA;AACA,kBAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA;;AA7BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA8BA,KA/BA;AAgCA,IAAA,EAhCA,gBAgCA;AACA,UAAA,KAAA,KAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA;AArCA,GAvCA;AA8EA,EAAA,OA9EA,qBA8EA;AACA,QAAA,KAAA,OAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA;AAlFA,CAAA","sourcesContent":["<template>\n    <v-container fluid fill-height style=\"background: linear-gradient(90deg, rgba(226,221,221,1) 0%, rgba(103,125,172,0.44441526610644255) 23%, rgba(21,101,192,1) 88%);\">\n        <v-row justify=\"center\">\n            <v-card elevation=\"15\" width=\"400\" style=\"border-radius:2em\">\n                <v-card-text>\n                    <v-form ref=\"form\" v-model=\"valid\">\n                        <v-container>\n                            <v-row>\n                                <v-col cols=\"12\">\n                                    <h1 class=\"primary--text\">Registro</h1>\n                                </v-col>\n                                <v-col cols=\"12\">\n                                    <v-text-field\n                                        v-model=\"name\"\n                                        :rules=\"nameRules\"\n                                        label=\"Nombre\"\n                                        dense\n                                        required\n                                    ></v-text-field>\n                                </v-col>\n                                <v-col cols=\"12\">\n                                    <v-text-field\n                                        v-model=\"email\"\n                                        :rules=\"emailRules\"\n                                        label=\"E-mail\"\n                                        required\n                                        dense\n                                    ></v-text-field>\n                                </v-col>\n                                <v-col cols=\"12\">\n                                    <v-text-field\n                                        v-model=\"phone\"\n                                        :rules=\"phoneRules\"\n                                        label=\"Teléfono\"\n                                        required\n                                        type=\"number\"\n                                        dense\n                                    ></v-text-field>\n                                </v-col>\n                                <v-col cols=\"12\">\n                                    <v-text-field\n                                        v-model=\"pass\"\n                                        :rules=\"passRules\"\n                                        label=\"Password\"\n                                        type=\"password\"\n                                        required\n                                        dense\n                                    ></v-text-field>\n                                </v-col>\n                                <v-col cols=\"12\">\n                                    <v-text-field\n                                        v-model=\"pass2\"\n                                        :rules=\"pass2Rules\"\n                                        label=\"Repita su Password\"\n                                        type=\"password\"\n                                        required\n                                        dense\n                                    ></v-text-field>\n                                </v-col>\n                                <v-col cols=\"12\" class=\"d-flex justify-center\">\n                                    <v-btn large color=\"secondary\" class=\"text-capitalize\" @click=\"register\">Registrarme</v-btn>\n                                </v-col>\n                            </v-row>\n                        </v-container>\n                    </v-form>\n                </v-card-text>\n            </v-card>\n        </v-row>\n        <Dialog\n            :message=\"message\"\n            :dialog=\"dialog\"\n            :redir=\"redir\"\n            @ok-template=\"ok\"\n        />\n    </v-container>\n</template>\n\n<script>\n\nimport qs from 'qs';\nimport Dialog from '@/components/DialogMessage';\nimport {mapState} from 'vuex';\n\nexport default {\n    components:{\n        Dialog\n    },\n    data() {\n        return{\n            valid:false,\n            name: '',\n            nameRules: [\n                v => !!v || 'El nombre es requerido',\n            ],\n            email: '',\n            emailRules: [\n                v => !!v || 'E-mail es requerido',\n                v => /.+@.+/.test(v) || 'E-mail invalido',\n            ],\n            phone: '',\n            phoneRules: [\n                v => !!v || 'El teléfono es requerido',\n            ],\n            pass: '',\n            passRules: [\n                v => !!v || 'El password es requerido',\n                v => /^(?=.*?[A-Z])(?=(.*[a-z]){1,})(?=(.*[\\d]){1,})(?=(.*[\\W]){1,})(?!.*\\s).{8,}$/.test(v) || 'La Contraseña es invalida debe incluir letras mayúsculas, minúsculas, números y caracteres especiales',\n            ],\n            pass2: '',\n            pass2Rules: [\n                v => !!v || 'El password es requerido',\n                v => v==this.pass || 'El password no es igual',\n            ],\n\n            dialog:false,\n            message:'',\n            redir:false,\n        }\n    },\n    computed:{\n        ...mapState(['session']),\n    },\n    methods:{\n        async register(){\n            if(this.$refs.form.validate()){\n                /*let resp=await fetch(this.$store.state.ip+'/csurf');\n                let {token}=resp.json();*/\n                let data={\n                    name:this.name,\n                    email:this.email,\n                    phone:this.phone,\n                    pass:this.pass,\n                }\n                this.axios({\n                    method: 'POST',\n                    data:qs.stringify(data),\n                    url:'/user/registerClient',\n                }).then(response=>{\n                    this.dialog=true;\n                    this.message=response.data.message+'';\n                    this.redir=response.data.status;\n                }).catch(err=>{\n                    this.dialog=true;\n                    if (err.response) {\n                        this.message=err.response.data.message;\n                    }else{\n                        this.message='Error en la respuesta';\n                    }\n                });\n\n            }else{\n                this.$refs.form.validate();\n            }\n        },\n        ok(){\n            if(this.redir){\n                this.$router.push('/login');\n            }\n            this.dialog=false;\n        }\n    },\n    created(){\n        if(this.session){\n            this.$router.push('/');\n        }\n    }\n}\n</script>"],"sourceRoot":"src/views/client"}]}