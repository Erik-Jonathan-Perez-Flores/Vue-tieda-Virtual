A;AACA,QAAA,MAAA,CAAA,kBAAA,GAAA,IAAA,CAAA,kBAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA,CAAA,QAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA,CAAA,SAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA,CAAA,OAAA,CAAA,IAAA;AACA,OAlBA,EAmBA,KAnBA,CAmBA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OArBA;AAsBA,KA1DA;AA2DA,IAAA,OA3DA,qBA2DA;AAAA;;AACA,UAAA,KAAA,OAAA,IAAA,IAAA,EAAA;AACA,YAAA,QAAA,GAAA,IAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,MAAA,CAAA,KAAA,EAAA,KAAA,OAAA;AACA,aAAA,KAAA,CAAA;AACA,UAAA,MAAA,EAAA,OADA;AAEA,UAAA,IAAA,EAAA,QAFA;AAGA,UAAA,GAAA,EAAA,eAAA,KAAA;AAHA,SAAA,EAIA,IAJA,CAIA,UAAA,QAAA;AAAA,iBAAA,QAAA,CAAA,IAAA;AAAA,SAJA,EAKA,IALA,CAKA,UAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,IAAA,CAAA,OAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,IAAA,CAAA,IAAA;AACA,SATA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,uBAAA;AACA,SAbA;AAcA,OAjBA,MAiBA;AACA,aAAA,MAAA,GAAA,IAAA;AACA,aAAA,OAAA,GAAA,8CAAA;AACA;AACA,KAjFA;AAkFA,IAAA,OAlFA,qBAkFA;AAAA;;AACA,WAAA,OAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,IAAA,EAAA,EAAA,CAAA,SAAA,CAAA;AACA,UAAA,EAAA,EAAA,KAAA;AADA,SAAA,CAFA;AAKA,QAAA,GAAA,EAAA;AALA,OAAA,EAMA,IANA,CAMA,UAAA,QAAA;AAAA,eAAA,QAAA,CAAA,IAAA;AAAA,OANA,EAOA,IAPA,CAOA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,gBAAA,CAAA,MAAA,CAAA,YAAA,GAAA,CAAA;AACA,OAZA,EAaA,KAbA,CAaA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,uBAAA;AACA,OAhBA;AAiBA;AArGA,IA7BA;AAoIA,EAAA,QAAA,kCACA,QAAA,CAAA,CAAA,cAAA,CAAA,CADA;AAEA,IAAA,IAFA,kBAEA;AACA,aAAA,KAAA,QAAA,CAAA,MAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AAAA,eAAA,CAAA,GAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,QAAA;AAAA,OAAA,EAAA,CAAA,CAAA;AACA,KAJA;AAKA,IAAA,QALA,sBAKA;AACA,aAAA,KAAA,OAAA,GAAA,KAAA,OAAA,GAAA,EAAA;AACA;AAPA,IApIA;AA8IA,EAAA,OA9IA,qBA8IA;AACA,SAAA,IAAA;AACA;AAhJA,CAAA","sourcesContent":["<template>\n    <v-container>\n        <v-row>\n            <v-col cols=\"12\">\n                <h1>Productos pedidos:</h1>\n                <p class=\"text-justify\">(El precio de los productos es el precio al momento de añadir al carrito del cliente)</p>\n            </v-col>\n            <v-col cols=\"12\">\n                <v-row justify=\"center\">\n                    <v-card\n                        color=\"gray\"\n                        width=\"250\" \n                        elevation=\"15\" \n                        class=\"ma-2\" v-for=\"(item,index) in products\" \n                        :key=\"index\"\n                        @click=\"toEdit(item.productId)\"\n                        style=\"border-radius:1em\"\n                    >\n                        <v-img\n                            :src=\"item.defaultImg\"\n                            height=\"200\"\n                        ></v-img>\n                        <v-card-text>\n                            <v-row>\n                                <v-col cols=\"12\" class=\"pa-1\">\n                                    <h3>Nombre: {{item.name}}</h3>\n                                </v-col>\n                                <v-col cols=\"12\" class=\"pa-1\">\n                                    <h3>Cantidad: {{item.quantity}}</h3>\n                                </v-col>\n                                <v-col cols=\"12\" class=\"pa-1\">\n                                    <h3 >Precio: {{item.price}}</h3>\n                                </v-col>\n                            </v-row>\n                        </v-card-text>\n                    </v-card>\n                </v-row>\n            </v-col>\n            <v-col cols=\"12\">\n                <v-row>\n                    <v-col cols=\"12\">\n                        <h1 class=\"accent--text text-center\">{{mont}} BOB</h1>\n                        <h3 class=\"text-center\">Monto Total para generar el código qr:</h3>\n                        <p class=\"text-center\">Puede enviar otro qr para editar/eliminar el enviado</p>\n                    </v-col>\n                    <v-col cols=\"12\" sm=\"6\" class=\"d-flex justify-center\">\n                        <Drag\n                            @drop-item=\"img\"\n                        />\n                    </v-col>\n                    <v-col cols=\"12\" sm=\"6\" class=\"d-flex justify-center\">\n                        <v-card style=\"border-radius:1em\"  class=\"pa-2\" elevation=\"15\" v-if=\"qrReturn!=''\">\n                            <h4 class=\"text-center accent--text\">Enviado a \"{{user}}\" por \"{{nameAdmin}}\"</h4>\n                            <v-img :src=\"qrReturn\" width=\"250\" height=\"250\" ></v-img>\n                        </v-card>\n                        <v-card style=\"border-radius:1em\" width=\"300\" height=\"300\" class=\"pa-1\" elevation=\"15\" v-else>\n                            <h4 class=\"text-center\">Aún no se envió un qr</h4>\n                            <v-row justify=\"center\">\n                                <v-icon size=\"280\" color=\"accent\">{{alertIcon}}</v-icon>\n                            </v-row>\n                        </v-card>\n                    </v-col>\n                    <v-col cols=\"12\" class=\"d-flex justify-center\">\n                        <v-btn large class=\"text-capitalize\" color=\"secondary\" @click=\"sending\">Enviar qr</v-btn>\n                    </v-col>\n                </v-row>\n            </v-col>\n            <v-col cols=\"12\" class=\"d-flex justify-center\">\n                <v-card width=\"400\" elevation=\"15\" color=\"primary\" dark class=\"pa-3\" style=\"border-radius:1em\">\n                    <v-card-text>\n                        <v-row>\n                            <v-col cols=\"12\">\n                                <h1 class=\"text-center\">Detalles del pedido</h1>\n                            </v-col>\n                            <v-col cols=\"12\" class=\"pa-1\">\n                                <h4>Tiempo: {{date}}</h4>\n                            </v-col>\n                            <v-col cols=\"12\" class=\"pa-1\">\n                                <h4>Total: {{mont}} BOB</h4>\n                            </v-col>\n                            <v-col cols=\"12\" class=\"pa-1\">\n                                <h4>Cliente: {{user}}</h4>\n                            </v-col>\n                            <v-col cols=\"12\" class=\"pa-1\">\n                                <h4>Ciudad: {{city}}</h4>\n                            </v-col>\n                            <v-col cols=\"12\" class=\"pa-1\">\n                                <h4>Calle: {{street}}</h4>\n                            </v-col>\n                            <v-col cols=\"12\" class=\"pa-1\">\n                                <h4>Teléfono: {{phone}}</h4>\n                            </v-col>\n                        </v-row>\n                    </v-card-text>\n                </v-card>\n            </v-col>\n            <v-col cols=\"12\">\n                <v-row>\n                    <v-col cols=\"12\">\n                        <h1>Respuesta del cliente</h1>\n                        <p>(La imagen aparecerá cuando el cliente envíe el recibo)</p>\n                    </v-col>\n                    <v-col cols=\"12\" class=\"d-flex justify-center\">\n                        <v-card style=\"border-radius:1em\" elevation=\"15\" class=\"pa-0\" v-if=\"receiptImage!=''\">\n                            <v-img :src=\"receiptImage\" width=\"500\" height=\"600\"></v-img>\n                        </v-card>\n                        <v-card style=\"border-radius:1em\" elevation=\"15\" class=\"pa-0\" v-else>\n                            <v-icon size=\"500\" color=\"accent\">{{alertIcon}}</v-icon>\n                        </v-card>\n                    </v-col>\n                    <v-col cols=\"12\">\n                        <h3>{{user}}: {{receiptDescription}}</h3>\n                    </v-col>\n                </v-row>\n            </v-col>\n            <v-col cols=\"12\">\n                <v-row>\n                    <v-col cols=\"12\">\n                        <h1>Comentarios del Admin al correo del cliente</h1>\n                        <p>Se recomienda solo enviar a lo mas 2 comentarios, para evitar confusiones escribale a su whatsapp {{phone}}</p>\n                    </v-col>\n                    <v-col cols=\"12\">\n                        <v-form ref=\"form\">\n                            <v-textarea\n                                outlined\n                                label=\"Envie una respuesta\"\n                                v-model=\"rexponseText\"\n                            >\n                            </v-textarea>\n                        </v-form>\n                    </v-col>\n                    <v-col cols=\"12\" class=\"d-flex justify-center\">\n                        <v-btn @click=\"sendComment\" large color=\"secondary\" class=\"text-capitalize\">Enviar Comentario</v-btn>\n                    </v-col>\n                    <v-col cols=\"12\">\n                        <v-card class=\"ma-1\" v-for=\"(item,j) in comments\" :key=\"j\" color=\"accent\">\n                            <v-card-text>\n                                <p class=\"white--text\">{{item.name}} : {{item.comment}}</p>\n                            </v-card-text>\n                        </v-card>\n                    </v-col>\n                </v-row>\n            </v-col>\n        </v-row>\n        <v-btn\n            fab\n            fixed\n            icon\n            bottom\n            right\n            class=\"pa-0\"\n            width=\"100\"\n            height=\"100\"\n            @click=\"dialog2=true\"\n            v-if=\"!sendState\"\n        >\n            <v-icon size=\"100\" color=\"secondary\">{{icon}}</v-icon>\n        </v-btn>\n        <v-dialog\n            v-model=\"dialog\"\n            width=\"400\"\n        >\n            <v-card class=\"pa-5\">\n                <v-card-text>\n                    <h3 class=\"text--primary text-center\">{{message}}</h3>\n                </v-card-text>\n                <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn @click=\"dialog=false\" color=\"primary\">Okay</v-btn>\n                    <v-spacer></v-spacer>\n                </v-card-actions>\n            </v-card>\n        </v-dialog>\n        <v-dialog\n            v-model=\"dialog2\"\n            width=\"400\"\n        >\n            <v-card class=\"pa-5\">\n                <v-card-text>\n                    <h3 class=\"text--primary text-center\">Confirmar envio del pedido</h3>\n                </v-card-text>\n                <v-card-actions>\n                    <v-btn small @click=\"dialog2=false\" color=\"primary\">Cancelar</v-btn>\n                    <v-spacer></v-spacer>\n                    <v-btn small @click=\"confirm\" color=\"primary\">Si</v-btn>\n                </v-card-actions>\n            </v-card>\n        </v-dialog>\n        <v-dialog\n            v-model=\"dialog3\"\n            width=\"400\"\n        >\n            <v-card class=\"pa-5\" color=\"primary\" dark style=\"border-radius:1em\">\n                <v-card-text>\n                    <v-row>\n                        <v-col cols=\"12\">\n                            <v-flex d-flex class=\"fill-height\">\n                                <v-row align-content=\"center\">\n                                    <v-col cols=\"12\" class=\"pa-1\">\n                                        <h4 class=\"text-center\" v-if=\"sendState\">Se confirmó el envio de este pedido</h4>\n                                        <h4 class=\"text-center\" v-else>Pulse el ícono para confirmar el envío del pedido al cliente {{user}}</h4>\n                                    </v-col>\n                                    <v-col cols=\"12\" class=\"pa-1 d-flex justify-center\">\n                                        <v-icon color=\"accent\" size=\"50\">{{icon}}</v-icon>\n                                    </v-col>\n                                </v-row>\n                            </v-flex>\n                        </v-col>\n                    </v-row>\n                </v-card-text>\n                <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn small @click=\"dialog3=false\" color=\"accent\">Okay</v-btn>\n                    <v-spacer></v-spacer>\n                </v-card-actions>\n            </v-card>\n        </v-dialog>\n    </v-container>\n</template>\n\n<script>\n\nimport Drag from '@/components/admin/DragImageAdmin';\nimport { mdiAlertCircleOutline } from '@mdi/js';\nimport qs from 'qs';\nimport { mdiSendCircle } from '@mdi/js';\nimport {mapMutations,mapState} from 'vuex';\n\nexport default {\n    props:['id'],\n    components:{\n        Drag\n    },\n    data() {\n        return{\n            dialog:false,\n            dialog2:false,\n            dialog3:true,\n            products:[],\n            date:null,\n            user:'',\n            city:'',\n            street:'',\n            phone:'',\n            qrImage:'',\n            imgFile:null,\n            receiptImage:'',\n            receiptDescription:'',\n            rexponseText:'',\n            comments:[],\n            alertIcon:mdiAlertCircleOutline,\n            message:'',\n            sendState:false,\n            icon:mdiSendCircle,\n            nameAdmin:'',\n        }\n    },\n    methods:{\n        ...mapMutations(['pushNotification']),\n        toEdit(id){\n            this.$router.push('/producteditadm/'+id);\n        },\n        img(img){\n            this.imgFile=img.image;\n        },\n        sendComment(){\n            if(this.$refs.form.validate()){\n                this.axios({\n                    method: 'PATCH',\n                    data:qs.stringify({\n                        message:this.rexponseText\n                    }),\n                    url:'/qr/mail/'+this.id,\n                }).then(response=>response.data)\n                .then(resp=>{\n                    this.dialog=true;\n                    this.message=resp.message+'';\n                    this.comments.push(resp.data);\n                    this.rexponseText='';\n                })\n                .catch(err=>{\n                    this.dialog=true;\n                    if (err.response) {\n                        this.message=err.response.data.message;\n                    }else{\n                        this.message='Error en la respuesta';\n                    }\n                });\n            }else{\n                this.$refs.form.validate()\n            }\n        },\n        init(){\n            this.axios({\n                method: 'GET',\n                data:null,\n                url:'/qr/product/'+this.id,\n            }).then(response=>response.data)\n            .then(resp=>{\n                this.products=resp.products;\n                this.user=resp.nameUser;\n                this.date=this.$moment(resp.date).fromNow();\n                this.city=resp.city;\n                this.street=resp.direction;\n                this.phone=resp.phoneConfirm;\n                this.qrImage=resp.qrImage;\n                this.receiptImage=resp.receiptImage;\n                this.receiptDescription=resp.receiptDescription;\n                this.comments=resp.comments;\n                this.sendState=resp.sendState;\n                this.nameAdmin=resp.adminId.name;\n            })\n            .catch(err=>{\n                console.log(err);\n            });\n        },\n        sending(){\n            if(this.imgFile!=null){\n                let formData=new FormData();\n                formData.append('img',this.imgFile);\n                this.axios({\n                    method: 'PATCH',\n                    data:formData,\n                    url:'/qr/imgQr/'+this.id,\n                }).then(response=>response.data)\n                .then(resp=>{\n                    this.dialog=true;\n                    this.message=resp.message;\n                    this.qrImage=resp.path;\n                })\n                .catch(err=>{\n                    this.dialog=true;\n                    this.message='Error en la respuesta';\n                });\n            }else{\n                this.dialog=true;\n                this.message='Se requiere enviar la imagen qr con el monto';\n            }\n        },\n        confirm(){\n            this.dialog2=false;\n            this.axios({\n                method: 'POST',\n                data:qs.stringify({\n                    id:this.id\n                }),\n                url:'/qr/confirm',\n            }).then(response=>response.data)\n            .then(resp=>{\n                this.dialog=true;\n                this.sendState=true;\n                this.message=resp.message;\n                this.pushNotification(this.notification-1);\n            })\n            .catch(err=>{\n                this.dialog=true;\n                this.message='Error en la respuesta';\n            });\n        }\n    },\n    computed:{\n        ...mapState(['notification']),\n        mont(){\n            return this.products.reduce((a,b)=>{return a+b.price*b.quantity},0);\n        },\n        qrReturn(){\n            return this.qrImage?this.qrImage:'';\n        }\n\n    },\n    created(){\n        this.init();\n    }\n}\n</script>"],"sourceRoot":"src/views/admin/data"}]}