IA,IAJA,CAIA,UAAA,QAAA;AAAA,eAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AAAA,OAJA,EAKA,IALA,CAKA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,EAAA,OAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,QAAA;AACA,OATA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAZA;AAaA;AAlBA,GArBA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,SAAA,IAAA;AACA;AA3CA,CAAA","sourcesContent":["<template>\n    <v-card \n        :color=\"state?'secondary':'accent'\" \n        style=\"border-radius:2em; cursor:pointer\"\n        @click=\"toQr\"\n    >\n        <v-card-text>\n            <v-row>\n                <v-col cols=\"12\">\n                    <h1 class=\"white--text text-center\">Total productos : {{products.length}}</h1>\n                </v-col>\n                <v-col cols=\"12\" sm=\"10\">\n                    <v-row >\n                        <v-col v-for=\"(item,index) in products\" :key=\"index\">\n                            <v-row justify=\"center\">\n                                <Product\n                                    :path=\"item.defaultImg\"\n                                    :price=\"item.price\"\n                                    :name=\"item.name\"\n                                    :_id=\"item._id\"\n                                    :quantity=\"item.quantity\"\n                                />\n                            </v-row>\n                        </v-col>\n                    </v-row>\n                </v-col>\n                <v-col cols=\"12\" sm=\"2\">\n                    <v-container fill-height>\n                        <v-row align-content=\"center\">\n                            <v-col cols=\"12\">\n                                <h3 class=\"white--text text-center\">Pedido : {{date}}</h3>\n                            </v-col>\n                            <v-col cols=\"12\" class=\"d-flex justify-center\">\n                                <v-row>\n                                    <v-col cols=\"12\" sm=\"6\" class=\"d-flex justify-center align-center\"><v-icon size=\"30\" color=\"white\">{{qr}}</v-icon></v-col>\n                                    <v-col cols=\"12\" sm=\"6\" class=\"d-flex justify-center align-center\"><v-icon size=\"30\" color=\"white\">{{click}}</v-icon></v-col>\n                                </v-row>\n                            </v-col>\n                        </v-row>\n                    </v-container>\n                </v-col>\n            </v-row>\n        </v-card-text>\n    </v-card>\n</template>\n\n<script>\n\n//this.$moment(new Date(2022, 11, 24, 10, 33)).fromNow()\n\nimport Product from './ProductQr.vue';\nimport { mdiQrcodeScan,mdiCursorDefaultClick } from '@mdi/js';\n\nexport default {\n    components:{\n        Product\n    },\n    data() {\n        return{\n            qr:mdiQrcodeScan,\n            click:mdiCursorDefaultClick,\n            products:[],\n            date:null,\n            state:null,\n        }\n    },\n    props:{\n        _id:{\n            type:String\n        }\n    },\n    mounted(){\n        \n    },\n    methods:{\n        toQr(){\n            this.$router.push('/qr/'+this._id);\n        },\n        init(){\n            this.axios({\n                method: 'GET',\n                data:null,\n                url:'/cart/getAIdQr/'+this._id,\n            }).then(response=>response.data.data)\n            .then(resp=>{\n                this.date=this.$moment(resp.date).fromNow();\n                this.state=resp.state;\n                this.products=resp.products;\n            })\n            .catch(err=>{\n                console.log(err);\n            });\n        }\n    },\n    created(){\n        this.init();\n    }\n}\n</script>"],"sourceRoot":"src/components/client"}]}